{
  "name": "vistral-supply",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:localhost": "next dev --webpack --hostname localhost --port 3003",
    "dev:simple": "next dev --webpack --port 3003",
    "dev:clean": "rm -rf .next && next dev --webpack",
    "dev:build": "npm run build && npm run start:dev",
    "kill:port": "lsof -ti:3002 | xargs kill -9 2>/dev/null || true",
    "kill:port:3003": "lsof -ti:3003 | xargs kill -9 2>/dev/null || true",
    "dev:force": "npm run kill:port && sleep 1 && npm run dev",
    "dev:staging": "next dev --env-file=.env.staging --port 3002",
    "build": "next build --webpack",
    "build:staging": "next build --webpack --env-file=.env.staging",
    "build:prod": "next build --webpack --env-file=.env.production",
    "start": "next start --port 3002",
    "start:dev": "next start --hostname localhost --port 3003",
    "start:staging": "next start --env-file=.env.staging --port 3002",
    "start:prod": "next start --env-file=.env.production --port 3002",
    "lint": "eslint",
    "test": "vitest",
    "create-test-users": "tsx scripts/create-test-users.ts",
    "assign-property": "tsx scripts/assign-property-to-supply.ts",
    "verify-assignment": "tsx scripts/verify-property-assignment.ts",
    "env:dev": "cp .env.example .env.local && echo '✅ Created .env.local - Please fill in your development credentials'",
    "env:staging": "cp .env.example .env.staging && echo '✅ Created .env.staging - Please fill in your staging credentials'",
    "env:prod": "cp .env.example .env.production && echo '✅ Created .env.production - Please fill in your production credentials'"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.10.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.87.0",
    "airtable": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^0.6.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.556.0",
    "mixpanel-browser": "^2.72.0",
    "next": "^16.0.10",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vistral-design-system": "github:angelvanegas1006/vistral-design-system",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.10",
    "lightningcss": "^1.31.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
